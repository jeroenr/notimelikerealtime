<script src="http://<%=server%>:4000/socket.io/socket.io.js"></script>
<script defer="defer">
  var socket = io.connect('http://<%=server%>:4000', { query: "id=<%=id%>" });
  socket.on('connect', function(){

    socket.on('permit_created', function(permitJson) {
      console.log(permitJson);
    });

    socket.on('disconnect', function(){});
  });
</script>